package Exercise1;

public class Person {
	private String name;
	private float age;
	/**
	 * to define name and age of person
	 */
	public String getName() {
		return name;
	}
	public void setName(String name){
		this.name=name;
	}
	public float getAge() {
		return age;
	}
	public void setAge(float age) {
		this.age=age;
	}

}